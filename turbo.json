{
  "$schema": "https://turborepo.org/schema.json",
  "baseBranch": "origin/main",
  "pipeline": {
    "build": {
      "outputs": [
        "dist/**",
        ".next/**"
      ],
      "dependsOn": [
        "^build"
      ]
    },
    "@twilight-toolkit/ui#build": {
      "outputs": [
        "dist/**"
      ]
    },
    "@twilight-toolkit/utils#build": {
      "outputs": [
        "dist/**"
      ]
    },
    "@ouorz/main#build": {
      "dependsOn": [
        "^build",
        "$GITHUB_TOKEN",
        "$NEXT_PUBLIC_LC_KEY",
        "$NEXT_PUBLIC_LC_ID",
        "$NEXT_PUBLIC_BUTTONDOWN_TOKEN",
        "$NEXT_PUBLIC_SENTRY_DSN",
        "$SENTRY_IGNORE_API_RESOLUTION_ERROR",
        "$SENTRY_AUTH_TOKEN",
        "$SENTRY_PROJECT",
        "$SENTRY_ORG",
        "$REVALIDATION_REQUEST_TOKEN"
      ],
      "outputs": [
        ".next/**"
      ]
    },
    "@ouorz/analytics#build": {
      "dependsOn": [
        "^build",
        "$DATABASE_URL",
        "$HASH_SALT",
        "$MAXMIND_LICENSE_KEY"
      ],
      "outputs": [
        ".next/**"
      ]
    },
    "test": {
      "dependsOn": [
        "^build"
      ],
      "outputs": []
    },
    "start": {
      "dependsOn": [
        "^build"
      ],
      "outputs": []
    },
    "lint": {
      "outputs": []
    },
    "dev": {
      "cache": false
    },
    "clean": {
      "cache": false
    },
    "upgrade": {
      "outputs": [
        "package.json"
      ]
    }
  },
  "globalDependencies": [
    "tsconfig.json",
    ".env.*"
  ],
  "remoteCache": {
    "signature": true
  }
}
